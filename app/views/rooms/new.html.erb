<p>新規ルーム作成</p>

<%=form_with model: @room, url: rooms_path, local: true do |f|%>

  <div class=''>
    <%= f.label :ルーム名, class: ''%>
  </div>
  <div class=''>
    <%= f.text_field :name, class: '', placeholder: 'ルーム名を入力してください'%>
  </div>
  <div class=''>
    <%= f.submit "ルームを作成する",class: 'btn' %>
  </div>
<% end %>